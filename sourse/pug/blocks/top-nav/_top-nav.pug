mixin top-nav() 
	// start top-nav
	+b.top-nav
		.container
			.row
				.col-md-auto.col
					+e.logo(href="#")
						img(src="img/logo.png", alt="")
				.col.d-none.d-md-block
					+b.nav-menu
						+e.link(href="#") КАРТА
						+e.link--current(href="#") мероприятия
						+e.link(href="#") блог
						+e.link(href="#") местоположение
				.col-auto
					+e.btn.popup-with-move-anim(href="#modal-order") 
						span.d-none.d-md-block записаться 
						span.d-md-none бесплатный билет
				.col-auto.d-none.d-md-block
					img(src="img/18+.png", alt="")

					
	// end top-nav